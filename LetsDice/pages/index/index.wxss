/**index.wxss**/
 .container{
  padding-top:10%;
} 
page{
  background: purple;
}
.foot{
  position: absolute;
  bottom: 0;
  right:0;
}
.foot text{
  color: #fff;
  font-size: 20rpx;
}
button{
  
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  bottom: 10%;
  width: 60%;
}
.primary{

  background: #128fdc;
  color: white;
}
.change{

  background: #ce4545;
  color: white;
}
.case{
  display: inline-block;
  position: relative;
}
.case1 , .case4{
  left: 50rpx;
}
.case2 , .case5{
  left: 200rpx;
}

.first-row , .second-row , .third-row{
  margin: 10rpx;
  display: block;
  width: 100%;
  height: 250rpx;
}
.face{
  position: relative;
  margin: 25rpx;
  padding: 30rpx;
  width: 130rpx;
  height: 130rpx;
  background-color: white;
  border-radius: 20%;
  border: 5rpx solid #888;
}

.pip{
  width: 30rpx;
  height: 30rpx;
  display: block;
  background-color:black;
  border-radius: 50%;
  position: absolute;
}
.corner-RD{
  bottom:30rpx;
  right:30rpx;
}
.center{
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.corner-RU{
  right: 30rpx;
  top: 30rpx;
}
.corner-LD{
  left: 30rpx;
  bottom: 30rpx;
}
.first .pip{

  width: 70rpx;
  height: 70rpx;
  background-color: red;
}

.second .pip{
  width: 45rpx;
  height: 45rpx;
}

.third .pip{
  width: 45rpx;
  height: 45rpx;
}

.fourth .pip{
  width: 45rpx;
  height: 45rpx;
  background-color: red;
}

.fifth .pip{
  width: 45rpx;
  height: 45rpx;
}

.sixth .pip{
  padding-top:-20rpx;
  width: 45rpx;
  height: 45rpx;
}

.six1{
  left: 35rpx;
  top: 20rpx;
}
.six2{
  right: 35rpx;
  top: 20rpx;
}
.six3{
  left: 35rpx;
  top:  75rpx;
}
.six4{
  right: 35rpx;
  top:  75rpx;
}
.six5{
  left:35rpx;
  top:130rpx;
}
.six6{
  right: 35rpx;
  top: 130rpx;
}

